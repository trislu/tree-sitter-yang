    "license": "MIT",
    "description": "Yang grammar for tree-sitter",
    "authors": [
      {
        "name": "trislu",
        "email": "lukai@mail.ustc.edu.cn",
        "url": ""
      }
    ],
    "links": {
      "repository": "https://github.com/trislu/tree-sitter-yang",
      "funding": ""
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
