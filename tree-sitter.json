  "grammars": [
    {
      "name": "yang",
      "camelcase": "Yang",
      "title": "Yang",
      "scope": "source.yang",
      "file-types": [
        "yang"
      ],
      "injection-regex": "^yang$",
      "class-name": "TreeSitterYang"
    }
