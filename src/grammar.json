{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "yang",
  "rules": {
    "yang": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "module"
        },
        {
          "type": "SYMBOL",
          "name": "submodule"
        }
      ]
    },
    "module": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "module"
        },
        {
          "type": "FIELD",
          "name": "arg",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_module_block"
        }
      ]
    },
    "_module_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "yang_version"
              },
              {
                "type": "SYMBOL",
                "name": "prefix"
              },
              {
                "type": "SYMBOL",
                "name": "namespace"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "prefix": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "prefix"
        },
        {
          "type": "FIELD",
          "name": "arg",
          "content": {
            "type": "SYMBOL",
            "name": "string"
          }
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "submodule": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "submodule"
        },
        {
          "type": "FIELD",
          "name": "arg",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_submodule_block"
        }
      ]
    },
    "_submodule_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "yang_version"
              },
              {
                "type": "SYMBOL",
                "name": "belongs_to"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "belongs_to": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "belongs-to"
        },
        {
          "type": "FIELD",
          "name": "arg",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "yang_version": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "yang-version"
        },
        {
          "type": "FIELD",
          "name": "arg",
          "content": {
            "type": "STRING",
            "value": "1.1"
          }
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "namespace": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "namespace"
        },
        {
          "type": "FIELD",
          "name": "arg",
          "content": {
            "type": "SYMBOL",
            "name": "string"
          }
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "//"
              },
              {
                "type": "PATTERN",
                "value": "(\\\\(.|\\r?\\n)|[^\\\\\\n])*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "/*"
              },
              {
                "type": "PATTERN",
                "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
              },
              {
                "type": "STRING",
                "value": "/"
              }
            ]
          }
        ]
      }
    },
    "identifier": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "[a-zA-Z_]"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "PATTERN",
              "value": "[a-zA-Z0-9-_.]"
            }
          }
        ]
      }
    },
    "_unescaped_string1": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "[^'\\\\]+"
        }
      }
    },
    "_unescaped_string2": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "[^\"\\\\]+"
        }
      }
    },
    "_escape_sequence": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\\"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "n"
              },
              {
                "type": "STRING",
                "value": "t"
              },
              {
                "type": "STRING",
                "value": "\""
              },
              {
                "type": "STRING",
                "value": "\\"
              }
            ]
          }
        ]
      }
    },
    "_single_quoted_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_unescaped_string1"
              },
              {
                "type": "SYMBOL",
                "name": "_escape_sequence"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "'"
        }
      ]
    },
    "_double_quoted_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_unescaped_string2"
              },
              {
                "type": "SYMBOL",
                "name": "_escape_sequence"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "_empty_string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "\"\""
        },
        {
          "type": "STRING",
          "value": "''"
        }
      ]
    },
    "_quoted_string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_single_quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "_double_quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "_empty_string"
        }
      ]
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s+"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}